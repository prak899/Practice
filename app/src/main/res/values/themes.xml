<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Practice" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimary</item>
        <item name="colorOnPrimary">@color/colorPrimary</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/colorPrimary</item>
        <item name="colorSecondaryVariant">@color/colorPrimary</item>
        <item name="colorOnSecondary">@color/colorPrimary</item>
    </style>

    <style name="NoActionBar" parent="Theme.Design.NoActionBar">

    </style>
    <style name="CustomSeekBar" parent="Widget.AppCompat.SeekBar">
        <!-- Customize the seek bar thumb -->
        <item name="android:thumb">@drawable/custom_seekbar_thumb</item>

        <!-- Customize the seek bar progress -->
        <item name="android:progressDrawable">@drawable/custom_seekbar_progress</item>

        <!-- Customize the seek bar track (background) -->
        <item name="android:track">@drawable/custom_seekbar_track</item>
        <item name="android:splitTrack">false</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminate">false</item>
    </style>

</resources>